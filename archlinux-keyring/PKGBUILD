# Maintainer: Severus <severus@theslinux.org>
# Original Maintainer: Pierre Schmitz <pierre@archlinux.de>

pkgdesc='Arch Linux PGP keyring'
arch=('any')
url='https://projects.archlinux.org/archlinux-keyring.git/'
license=('GPL')
install="${pkgbase}.install"
source=("ftp://ftp.archlinux.org/other/${pkgbase}/${pkgbase}-${pkgver}.tar.gz"
        "ftp://ftp.archlinux.org/other/${pkgbase}/${pkgbase}-${pkgver}.tar.gz.sig")

package() {
	cd "${srcdir}/${pkgbase}-${pkgver}"
	make PREFIX=/usr DESTDIR=${pkgdir} install
}
