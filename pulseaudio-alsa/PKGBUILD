pkgdesc="ALSA Configuration for PulseAudio"
arch=(any)
url="http://www.pulseaudio.org"
license=(GPL)
depends=("alsa-plugins>=1.0.25" pulseaudio)
backup=(etc/asound.conf)
source=(asound.conf)

package() {
  install -Dm644 asound.conf "$pkgdir/etc/asound.conf"
}
